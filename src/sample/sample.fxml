<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <VBox prefHeight="640.0" prefWidth="360.0">
            <children>
                <ImageView pickOnBounds="true" preserveRatio="true" fx:id="my_image">
                    <image>
                        <Image url="https://img-blog.csdnimg.cn/20190125135919960.png"/>
                    </image>
                </ImageView>
            </children>
        </VBox>
    </children>
</GridPane>
